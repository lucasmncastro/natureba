function _init()
 x=23
 y=5
 f=false
 s=1
 sp=1
 t=0
 tic=0
 
 -- 0 - title
 -- 1 - running 
 state=0
 
 title="natureba"
 lives=0
 
 mom_prog=0
 flowers=0
 goal=20
end

function _update()
 update_utils()
 if state==0 then
  update_title()
 end
 if state==1 then
  update_game()
 end
 update_camera()
end

function update_game()
 if btnp(⬆️) then
 	ny-=1
 end
 if btnp(⬇️) then
 	ny+=1
 end
 if btnp(➡️) then
 	nx+=1
  f=false
 end
 if btnp(⬅️) then
 	nx-=1
 	f=true
 end
 if btnp(⬅️) or btnp(➡️) or
  btnp(⬆️) or btnp(⬇️) then
  msg=nil
 end

 -- block
 cell=mget(nx,ny)
 if fget(cell,0) then
  -- frog over water
  if not (fget(cell,4) and sp==52) then
  	nx=x
  	ny=y
  	sfx(0)
  end
 end
 
 -- when hit flag 3
 -- trigger event.
 if fget(cell,3) then
  trigger(cell,nx,ny)
 end
 
 -- limits
 if nx<0 then
  nx=x
 end
 if ny<0 then
  ny=y
 end
 x=nx
 y=ny
end

function trigger(i,_x,_y)
 -- mom dialog
 local mom={4,5}
 if del(mom,i) then
  if mom_prog==0 and flowers==0 then
   show_text("go safe, son...\ni'm waiting for you.")
   mom_prog=1
  elseif mom_prog==1 then
   show_text("please, get flowers!")
   mom_prog=2  
  elseif mom_prog==2 then
   show_text("all the flowers in this world!")
   mom_prog=1
  end
 end
 
 local flw={16,32,48,64} 
 if del(flw,i) then
  if (i==16) flowers+=1
  if (i==48) flowers+=1
  if (i==32) flowers+=3
  
  sfx(2)
  mset(_x,_y,nil) 
  if (i==64) then
   flowers+=1
   mset(_x,_y,34)
  end
 end
 
 local flw={36,37} 
 if del(flw,i) then
  if sp==52 then
   show_text("no! is'nt you.")
  else
   show_text("where is my frog?")  
  end
 end

 local frg={52,53}
 if del(frg,i) then
  show_text("❎ and hit to turn a frog.")
 end

 local frg={52,53} 
 if del(frg,i) and btn(❎) then
  transform(52,x,y)
 end


 if flowers==goal then
  state=2
 end
end

function transform(s,_x,_y)
	sp=s
end

function show_text(_txt)
 msg=_txt
end

function _draw()
	cls(0)

	camera(cx,cy) 
	map()
	spr(sp,x*8,y*8,1,1,f)

 if state==0 then
  rectfill(cx,cy+54,cx+128,cy+79,7)
 	print(title,cx+60-#title*2,cy+60,0)
  start_title="press ❎ to start"
  print(start_title,cx+60-#start_title*2,cy+70,6)
 end

 if cell then
  --print(cell,cx,cy,11)
 end

 -- status bar
 if state!=0 then
  -- todo align right
	 --for i=0,lives-1 do
	 -- spr(3,cx+8*i,cy)
	 --end
	 rectfill(cx,cy,cx+128,cy+5,0)
	 spr(48,cx,cy-2)
	 print(flowers.."/"..goal, cx+7,cy,7)
 end
 
 if msg then
  rectfill(cx,cy+56,cx+128,cy+75,7)
 	print(msg,cx+2,cy+60,0)
 end
 
 if state==2 then
  rectfill(cx,cy+56,cx+128,cy+68,7)
 	print("congrats!",cx+5, cy+60,tic*14)
 end
end
-->8
-- utils

function update_title()
 if btnp(❎) then
  state=1
  music(0)
 end
end

function update_camera()
	cx=(x*8\128)*128
	cy=(y*8\128)*128
end

function update_utils()
 nx=x
 ny=y

 t+=1
 if t%10==0 then
  tic+=1
  if tic==2 then
   tic=0
  end  
  change_sprites()
 end
end

function change_sprites()
 for ix=0,127 do
  for iy=0,127 do
   local ic = mget(ix+1,iy)
   if tic==1 then
	   if fget(ic,2) then
	    mset(ix+1,iy,ic-1)
	   end
	  else
	   if fget(ic,1) then
	    mset(ix+1,iy,ic+1)
	   end
	  end
  end
 end
end
